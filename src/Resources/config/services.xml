<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service alias="Nucleos\DompdfBundle\Wrapper\DompdfWrapper" id="dompdf" public="true"/>
    <service alias="Nucleos\DompdfBundle\Factory\DompdfFactory" id="dompdf.factory" public="true"/>
    <service alias="Nucleos\DompdfBundle\Wrapper\DompdfWrapper" id="Nucleos\DompdfBundle\Wrapper\DompdfWrapperInterface" public="true"/>
    <service alias="Nucleos\DompdfBundle\Factory\DompdfFactory" id="Nucleos\DompdfBundle\Factory\DompdfFactoryInterface" public="true"/>
    <service id="Nucleos\DompdfBundle\Wrapper\DompdfWrapper">
      <argument type="service" id="dompdf.factory"/>
      <argument type="service" id="event_dispatcher" on-invalid="null"/>
    </service>
    <service id="Nucleos\DompdfBundle\Factory\DompdfFactory">
      <argument>%nucleos_dompdf.options%</argument>
    </service>
  </services>
</container>
